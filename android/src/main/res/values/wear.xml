<resources>
    <!-- This file is needed to tell to the Wear Companion app that
    this handheld version of the app provides specific capabilities
    That way, the Wear app can send messages to connected devices
    which only have those specific capabilities.
    -->

    <string-array name="android_wear_capabilities">
        <item>send_me_email</item>
    </string-array>


</resources>